body{
  background-color: #f7f7f7
  margin 0px
}
.mtop16{
  margin-top: 16px
}
.box {
  background-color: #fff
  border-radius: 8px
  border 1px solid #d6d6d6
  position: absolute
  width: 500px
}
.btn-primary{
  background-color:#1A4983
  border: 0px
  width: 100%
}
.inside{
  padding: 10px
}
.imagenLogo{
  width: 20%
  display: block
  margin: 0px auto
}
.header{
  background-color:#1A4983
  border-radius: 8px 8px 0px 0px
  padding: 16px
}
.footer{
  text-align: center
  a{
    display:inline-block
  }
}
.box_login{
  left calc(50% - 250px)
  top calc(50% - 200px)
}
.box_register{
  left calc(50% - 250px)
  top calc(50% - 300spx)
}
*{
  margin: 0px
  padding: 0px
  font-family: helvetica
}
div#banner{
  width: 100%
  height: 100px
  background-color: white
}
